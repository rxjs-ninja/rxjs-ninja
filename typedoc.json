{
  "name": "RxJS Ninja",
  "mode": "modules",
  "out": "docs",
  "exclude": ["**/*.spec.ts", "tools/**/*"],
  "excludeExternals": true,
  "excludeNotExported": true,
  "excludePrivate": true,
  "stripInternal": false,
  "readme": "README.md",
  "includes": "libs/rxjs",
  "tsconfig": "tsconfig.base.json",
  "theme": "./node_modules/typedoc-ninja-theme/bin/default",
  "favicon": "assets/favicon.ico",
  "cname": "rxjs.ninja",
  "plugin": [
    "typedoc-ninja-theme",
    "typedoc-plugin-external-module-name",
    "typedoc-plugin-extras",
    "typedoc-plugin-pages",
    "typedoc-plugin-ghpages-cname",
    "typedoc-plugin-replace-in-comments"
  ],
  "links": [
    {
      "label": "Github",
      "url": "https://github.com/rxjs-ninja/rxjs-ninja"
    },
    {
      "label": "NPM",
      "url": "https://www.npmjs.com/org/rxjs-ninja"
    }
  ],
  "source": [
    {
      "path": "https://github.com/rxjs-ninja/rxjs-ninja/blob/main/",
      "line": "L"
    }
  ],
  "pages": {
    "replaceGlobalsPage": true,
    "reflectionNavigationTitle": "RxJS Ninja APIs",
    "enableSearch": false,
    "groups": [
      {
        "title": "Package Docs",
        "output": "packages",
        "pages": [
          {
            "title": "Array",
            "source": "libs/rxjs/array/README.md",
            "output": "array",
            "children": [
              {
                "title": "Changelog",
                "source": "libs/rxjs/array/CHANGELOG.md",
                "output": "changelog"
              }
            ]
          },
          {
            "title": "Boolean",
            "source": "libs/rxjs/boolean/README.md",
            "output": "boolean",
            "children": [
              {
                "title": "Changelog",
                "source": "libs/rxjs/boolean/CHANGELOG.md",
                "output": "changelog"
              }
            ]
          },
          {
            "title": "Number",
            "source": "libs/rxjs/number/README.md",
            "output": "number",
            "children": [
              {
                "title": "Changelog",
                "source": "libs/rxjs/number/CHANGELOG.md",
                "output": "changelog"
              }
            ]
          },
          {
            "title": "Random",
            "source": "libs/rxjs/random/README.md",
            "output": "random",
            "children": [
              {
                "title": "Changelog",
                "source": "libs/rxjs/random/CHANGELOG.md",
                "output": "changelog"
              }
            ]
          },
          {
            "title": "String",
            "source": "libs/rxjs/string/README.md",
            "output": "string",
            "children": [
              {
                "title": "Changelog",
                "source": "libs/rxjs/string/CHANGELOG.md",
                "output": "changelog"
              }
            ]
          },
          {
            "title": "Utility",
            "source": "libs/rxjs/utility/README.md",
            "output": "utility",
            "children": [
              {
                "title": "Changelog",
                "source": "libs/rxjs/utility/CHANGELOG.md",
                "output": "changelog"
              }
            ]
          }
        ]
      }
    ]
  },
  "replace-in-comments-config": [
    {
      "pattern": "(Observables?)",
      "flags": "gm",
      "replace": "[$1](https://rxjs.dev/api/index/class/Observable)"
    },
    {
      "pattern": "RxJS",
      "flags": "gm",
      "replace": "[RxJS](https://rxjs.dev)"
    },
    {
      "pattern": "RegExp",
      "flags": "gm",
      "replace": "[RegExp](https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/RegExp)"
    },
    {
      "pattern": "(Intl\\.(\\w+))",
      "flags": "gm",
      "replace": "[$1](https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Intl/$2)"
    },
    {
      "pattern": "(Number\\.(\\w+))",
      "flags": "gm",
      "replace": "[$1](https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Number/$2)"
    },
    {
      "pattern": "(String\\.(\\w+))",
      "flags": "gm",
      "replace": "[$1](https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/String/$2)"
    },
    {
      "pattern": "(Array\\.(\\w+))",
      "flags": "gm",
      "replace": "[$1](https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Array/$2)"
    },
    {
      "pattern": "(Math\\.(\\w+))",
      "flags": "gm",
      "replace": "[$1](https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Math/$2)"
    },
    {
      "pattern": "(Crypto\\.(\\w+))",
      "flags": "gm",
      "replace": "[$1](https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Crypto/$2)"
    }
  ]
}
