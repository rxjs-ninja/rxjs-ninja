{
  "name": "@rxjs-ninja/rxjs-ninja",
  "deacription": "This is the tooling for the rxjs ninja repo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "nx build",
    "cleanup:branches": "git pull --prune && git branch -vv | grep ': gone]'|  grep -v \"\\*\" | awk '{ print $1; }' | xargs -r git branch -D",
    "cleanup:workspace": "nxpm-stack lint",
    "coverage": "codecov --disable=gcov --yml=codecov.yaml",
    "dep-graph": "nx dep-graph",
    "docs": "typedoc",
    "e2e": "nx e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "lint": "nx workspace-lint && nx lint",
    "nx": "nx",
    "start": "nx serve",
    "test": "nx test",
    "test:cov": "nx test --codeCoverage=true",
    "update": "nx migrate latest",
    "watch": "onchange -d 20000 'libs/**/*.ts' 'libs/**/*.md' -- npm run docs",
    "workspace-generator": "nx workspace-generator"
  },
  "private": true,
  "dependencies": {
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@nrwl/cli": "11.2.0",
    "@nrwl/eslint-plugin-nx": "11.2.0",
    "@nrwl/jest": "11.2.0",
    "@nrwl/node": "11.2.0",
    "@nrwl/nx-cloud": "11.1.2",
    "@nrwl/web": "11.2.0",
    "@nrwl/workspace": "11.2.0",
    "@nxpm/cli": "^4.11.1",
    "@trust/webcrypto": "^0.9.2",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.22",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "codecov": "^3.8.1",
    "dotenv": "8.2.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eventsourcemock": "^2.0.0",
    "jest": "26.6.3",
    "jest-marbles": "^2.5.1",
    "onchange": "^7.1.0",
    "prettier": "2.2.1",
    "rollup": "^2.38.1",
    "rxjs-marbles": "^6.0.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "tslib": "^2.1.0",
    "tslint": "6.1.3",
    "typedoc": "^0.20.19",
    "typedoc-ninja-theme": "1.0.0-rc.16",
    "typedoc-plugin-extras": "^1.1.7",
    "typedoc-plugin-ghpages-cname": "^1.0.1",
    "typedoc-plugin-pages-fork": "^0.0.1",
    "typedoc-plugin-replace-in-comments": "^2.2.0",
    "typescript": "~4.1.3",
    "web-streams-polyfill": "^3.0.1"
  }
}
