{
  "name": "RxJS Ninja",
  "mode": "modules",
  "out": "docs",
  "exclude": ["**/*.spec.ts", "tools/**/*"],
  "excludeExternals": true,
  "excludeNotExported": true,
  "excludePrivate": true,
  "stripInternal": true,
  "readme": "README.md",
  "includes": "libs/rxjs",
  "tsconfig": "tsconfig.base.json",
  "theme": "./node_modules/typedoc-loopingz-theme/bin/default",
  "favicon": "assets/favicon.ico",
  "cname": "rxjs.ninja",
  "plugin": [
    "typedoc-loopingz-theme",
    "typedoc-plugin-external-module-name",
    "typedoc-plugin-extras",
    "typedoc-plugin-ghpages-cname"
  ],
  "links": [
    {
      "label": "Github",
      "url": "https://github.com/tanepiper/rxjs-ninja"
    },
    {
      "label": "NPM",
      "url": "https://www.npmjs.com/org/rxjs-ninja"
    }
  ],
  "source": [
    {
      "path": "https://github.com/rxjs-ninja/rxjs-ninja/blob/master/",
      "line": "L"
    }
  ]
}
